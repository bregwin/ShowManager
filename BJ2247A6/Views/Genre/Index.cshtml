@model IEnumerable<BJ2247A5.Models.GenreBaseViewModel>

@{
    ViewBag.Title = "Genre";
}

<h2>Genre List</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
